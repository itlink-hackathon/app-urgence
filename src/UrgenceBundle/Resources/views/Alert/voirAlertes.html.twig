{% extends 'UrgenceBundle::layout.html.twig' %}

{% block title %}Voir les alertes{% endblock %}

{% block content %}
<div class="container-fluid">
    <div class="row">
        <div class="col-xs-12 col-md-9 col-lg-9">
            <div id="map-voir-alertes" style="padding-left: 0; padding-right: 0;"></div>
        </div>
        <div class="col-xs-12 col-md-3 col-lg-3 cards-container"></div>
    </div>
</div>
<script type="text/javascript">
function initMap() {
  var map = mapModule.init('map-voir-alertes');
  mapModule.loadData(map);
  setInterval(function() { mapModule.loadData(map); }, 2000);
};
</script>
<script async defer
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBspTNgVFapTrsO_xYRY5Zzbx1nfDJPVXg&callback=initMap">
</script>
{% endblock %}